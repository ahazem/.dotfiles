# bash_exports

# Make vim the default editor.
export EDITOR="vim"

# Always include rubygems while executing ruby scripts.
export RUBYOPT="rubygems"

# Node Modules
export NODE_PATH="/usr/local/lib/node_modules"

# Go
export GOPATH="/Users/ahazem/.go"
export GOBIN="/Users/ahazem/.go/bin"

# Rails Assets
export DEV_ASSET_DEBUG="false"

# Local
export API_ADDRESS="http://localhost:9090"
export ASSETS_ADDRESS="http://localhost:9292"

# Python (Locale Issues)
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# Vault
export VAULT_ADDR='http://127.0.0.1:8200'
