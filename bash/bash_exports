# bash_exports

# Make vim the default editor.
export EDITOR="vim"

# Node Modules
export NODE_PATH="/usr/local/lib/node_modules"

# Go
export GOPATH=~/go
export GOBIN=/usr/local/go/bin

# Rails Assets
export DEV_ASSET_DEBUG="false"

# Python (Locale Issues)
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# ElasticSearch
export ES_HOME="~/documents/elasticsearch"

# GH
export GH_BASE_DIR="$HOME/documents/dev"
